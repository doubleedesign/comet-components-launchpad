{
    "name": "doubleedesign/comet-standalone-launchpad",
    "description": "Base package of common dependencies for Comet Components standalone packages",
    "version": "0.0.2",
    "homepage": "https://cometcomponents.io",
    "authors": [
        {
            "name": "Leesa Ward",
            "email": "leesa@doubleedesign.com.au"
        }
    ],
    "autoload": {
        "classmap": [
            "dist/src/components/",
            "dist/src/components/**/"
        ],
        "psr-4": {
            "Doubleedesign\\Comet\\Core\\": [
                "dist/src/base/types/",
                "dist/src/base/types/",
                "dist/src/base/attributes/",
                "dist/src/base/traits/",
                "dist/src/base/components/",
                "dist/src/base/",
                "dist/src/services/"
            ]
        }
    },
    "require": {
        "illuminate/view": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/events": "^10.0",
        "illuminate/container": "^10.0",
        "illuminate/support": "^10.0",
        "ezyang/htmlpurifier": "^4.18"
    }
}